{
    "name": "lisa-bot",
    "version": "8.0.1",
    "private": true,
    "description": "A discord bot with miscellaneous features",
    "main": "./dist/src/main.js",
    "types": "./dist/src/main.d.ts",
    "scripts": {
        "start": "npm-run-all build serve",
        "serve": "node dist/src/main.js",
        "dist": "npm-run-all clean lint build test",
        "clean": "rimraf ./dist ./spec/dist",
        "build": "tsc --build ./tsconfig.json",
        "watch": "tsc --build ./tsconfig.json -w",
        "lint": "eslint ./src/ ./spec/src/ --ext .ts",
        "fix": "eslint ./src/ ./spec/src/ --ext .ts --fix",
        "test": "npm-run-all build test:jasmine",
        "test:jasmine": "jasmine --require=source-map-support/register"
    },
    "keywords": [
        "discord",
        "bot"
    ],
    "homepage": "https://github.com/FelixRilling/lisa-bot",
    "author": {
        "name": "Felix Rilling",
        "email": "contact@rilling.dev",
        "url": "https://rilling.dev"
    },
    "repository": "https://github.com/FelixRilling/lisa-bot",
    "license": "MIT",
    "dependencies": {
        "chevronjs": "^12.0.0",
        "discord.js": "github:discordjs/discord.js#b4e56d3e0e8c3bf242d6bf52af03b0826af62bd8",
        "discord.js-commando": "github:discordjs/Commando#4beeec829ed25fc2c6ec0896edf7e0ac4d8bfcf0",
        "fs-extra": "^8.1.0",
        "lightdash": "^11.1.2",
        "lodash": "^4.17.15",
        "moment": "^2.24.0",
        "rxjs": "^6.5.4",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@types/fs-extra": "^8.0.1",
        "@types/jasmine": "^3.5.2",
        "@types/lodash": "^4.14.149",
        "@types/node": "^13.7.0",
        "@types/ws": "^7.2.1",
        "@typescript-eslint/eslint-plugin": "^2.18.0",
        "@typescript-eslint/parser": "^2.18.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-prettier": "^3.1.2",
        "jasmine": "^3.5.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.19.1",
        "rimraf": "^3.0.1",
        "source-map-support": "^0.5.16",
        "typescript": "^3.7.5"
    }
}
